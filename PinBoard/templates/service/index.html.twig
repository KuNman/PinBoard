{% extends 'base.html.twig' %}
    {% block content %}
        <div class="content">
            {% include '/service/header.html.twig' %}
            {{ form_start(form) }}
            <div class="content-content">
                <div class="welcome">
                    <div id="#typed-welcome">I'am looking for</div>
                </div>
                <div class="index-form">
                    <div class="name">
                    {{ form_widget(form.name) }}
                    </div>
                    <div class="place" id="locationField">
                    in {{ include('maps.html.twig') }}{{ form_widget(form.place) }}
                    </div>
                </div>
            </div>
            <div class="content-button">
                <div class="button">
                    {{ form_widget(form.submit)}}
                </div>
            </div>
            {{ form_end(form) }}
        </div>
    {% endblock content%}
    {% block javascripts %}
    <script>
        document.getElementById("form_place").value = '';
    </script>
    {% endblock %}
